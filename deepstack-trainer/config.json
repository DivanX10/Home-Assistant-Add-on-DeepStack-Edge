{
	"name": "DeepStack Trainer",
	"version": "2.2.0",
	"url": "https://github.com/t0mer/deepstack-trainer",
	"slug": "deepstack-trainer",
	"description": "DeepStack Trainer - клиент c графическим интерфейсом",
	"webui": "http://[HOST]:[PORT:8080]",
	"arch": ["amd64"],
	"startup": "application",
	"boot": "auto",
	"map": ["config"],
	"ports": {
		"8080/tcp": 7100
	},
	"ports_description": {
		"8080/tcp": "Optional. Port of your deepstack server"
	},
	"environment": {
		"DEEPSTACK_HOST_ADDRESS": "http://homeassistant:7000"
	},
	"image": "techblog/deepstack-trainer",
	"options": {
		"data_path": "/config/zigbee2mqtt",
		"VISION-FACE": "bool",
		"VISION-DETECTION": "bool",
		"VISION-SCENE": "bool",
		"MODELSTORE-DETECTION": "bool"
	},
	"schema": {
		"data_path": "str",
		"VISION-FACE": "bool",
		"VISION-DETECTION": "bool",
		"VISION-SCENE": "bool",
		"MODELSTORE-DETECTION": "bool"
	}
}
