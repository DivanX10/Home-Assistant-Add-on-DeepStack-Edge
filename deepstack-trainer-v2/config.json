{
  "name": "DeepStack Trainer V2",
  "version": "2.2.0",
  "url": "https://github.com/t0mer/deepstack-trainer",
  "panel_icon": "mdi:face-recognition",
  "slug": "deepstack-trainer",
  "webui": "http://[HOST]:[PORT:8080]",
  "description": "DeepStack Trainer - клиент c графическим интерфейсом",
  "arch": ["amd64"],
  "startup": "application",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 8080,
  "ports": {
    "8080/tcp": 7100
  },
  "ports_description": {
    "8080/tcp": "Web interface (not required for Home Assistant ingress)"
  },
  "map": ["config:rw"],
  "environment": {
    "HA_ADDON": "true",
    "DEEPSTACK_HOST_ADDRESS": "http://homeassistant:7000"
  },
  "options": {
    "STORAGE_PATH": "/opt/trainer/photos/uploads",
    "CONFIG_PATH": "/config/deepstack-trainer",
    "SECRETS_PATH": "/config"
  },
  "schema": {
    "STORAGE_PATH": "str",
    "CONFIG_PATH": "str",
    "SECRETS_PATH": "str"
  },
  "image": "techblog/deepstack-trainer"
}
